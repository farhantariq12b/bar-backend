{
	//  "parser": "@typescript-eslint/parser",
	"extends": [
		"airbnb/base",
		"plugin:import/errors",
		"plugin:import/warnings",
		"prettier"
	],
	"parserOptions": {
		"ecmaVersion": 2021,
		"sourceType": "module"
	},
	"plugins": ["prettier"],
	"rules": {
		"sort-imports": "off",
		"prettier/prettier": ["warn"],
		"semi": ["warn", "always"],
		"object-curly-spacing": ["error", "always"],
		"camelcase": "off",
		"no-underscore-dangle": "off",
		"no-shadow": "off",
		"no-new": 0,
		"no-param-reassign": "off",
		"func-names": "off",
		"quotes": [0, "single", { "avoidEscape": true }],
		"class-methods-use-this": "off",
		"new-cap": "off",
		"object-shorthand": "off",
		"import/extensions": [
			"error",
			"ignorePackages",
			{
				"js": "never",
				"jsx": "never",
				"ts": "never",
				"tsx": "never"
			}
		]
	}
}
